<div class="pmd-table-card pmd-card pmd-z-depth">
  <table id="cars_table" class="table pmd-table table-striped">
    <thead>
    <tr>
      <th>Name</th>
      <th>Dealership</th>
      <th>Price</th>
      <th>Status</th>
      <th>Actions</th>
    </tr>
    </thead>
    <tbody>
      <% if @objects.each do |object| %>
        <tr>
          <td><%= object.name %></td>
          <td><%= object.dealership.name %></td>
          <td><%= object.price %></td>
          <td><%= object.status.capitalize %></td>
          <td class="list-group-item">
            <%= link_to edit_car_path(object), id: 'edit',
                        data: { "toggle": "tooltip", "container": "body", "placement": "top", "original-title": "Edit" } do %>
              <i class="material-icons pmd-sm">create</i>
            <% end %>
            <%= render 'layouts/common/icon_links/destroy',
                      link: car_path(object) %>
          </td>
        </tr>
      <% end.empty? %>
        <tr>
          <td colspan=11 class='center'>
            <h3 class="list-group-item-heading">
              <%= t('core.empty') %>
            </h3>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>